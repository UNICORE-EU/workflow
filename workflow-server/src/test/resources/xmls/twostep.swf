<s:Workflow xmlns:s="http://www.chemomentum.org/workflow/simple"
          xmlns:jsdl="http://schemas.ggf.org/jsdl/2005/11/jsdl"
          xmlns:jsdl1="http://schemas.ggf.org/jsdl/2005/11/jsdl-posix">
  <s:Documentation>
    <s:Creator>Chemomentum</s:Creator>
    <s:CreationDate>2007-10-04T16:48:20.017+02:00</s:CreationDate>
    <s:Comment>Simple chain:
     1.) 'date' with a stage-out,
     2.) cat the output from the first step.
    </s:Comment>
  </s:Documentation>
  <s:Activity Id="date" Name="JSDL">
   <s:JSDL>
      <jsdl:JobDescription>
        <jsdl:Application>
          <jsdl:ApplicationName>Date</jsdl:ApplicationName>
        </jsdl:Application>
       <jsdl:DataStaging>
         <jsdl:FileName>stdout</jsdl:FileName>
         <jsdl:Target>
           <jsdl:URI>c9m:$WORKFLOW_ID/date.out</jsdl:URI>
         </jsdl:Target>
         </jsdl:DataStaging>
      </jsdl:JobDescription>
    </s:JSDL>
   </s:Activity>
  <s:Activity Id="cat" Name="JSDL">
   <s:JSDL>
      <jsdl:JobDescription>
        <jsdl:Application>
          <jsdl1:POSIXApplication>
             <jsdl1:Executable>/bin/cat</jsdl1:Executable>
             <jsdl1:Argument>first_step_output</jsdl1:Argument>
          </jsdl1:POSIXApplication>
        </jsdl:Application>
        <jsdl:DataStaging>
        <jsdl:FileName>first_step_output</jsdl:FileName>
        <jsdl:Source>
          <jsdl:URI>c9m:$WORKFLOW_ID/date.out</jsdl:URI>
        </jsdl:Source>
        </jsdl:DataStaging>
       <jsdl:DataStaging>
         <jsdl:FileName>stdout</jsdl:FileName>
         <jsdl:Target>
           <jsdl:URI>c9m:$WORKFLOW_ID/final.out</jsdl:URI>
         </jsdl:Target>
         </jsdl:DataStaging>
      </jsdl:JobDescription>
    </s:JSDL>
   </s:Activity>

  <s:Transition Id="date-cat" From="date" To="cat"/>

</s:Workflow>
