<s:Workflow xmlns:s="http://www.chemomentum.org/workflow/simple"
 xmlns:jsdl="http://schemas.ggf.org/jsdl/2005/11/jsdl"
 xmlns:jsdl1="http://schemas.ggf.org/jsdl/2005/11/jsdl-posix"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
>

  <s:Activity Id="a1" Type="JSDL">
    <s:JSDL>
      <jsdl:JobDescription>
        <jsdl:Application>
          <jsdl:ApplicationName>Date</jsdl:ApplicationName>
        </jsdl:Application>
      </jsdl:JobDescription>
    </s:JSDL>
  </s:Activity>
 
  <s:SubWorkflow Id="s1">
 
   <s:Activity Id="a2" Type="JSDL">
    <s:JSDL>
      <jsdl:JobDescription>
        <jsdl:Application>
          <jsdl:ApplicationName>Date</jsdl:ApplicationName>
        </jsdl:Application>
      </jsdl:JobDescription>
    </s:JSDL>
   </s:Activity>
 
   <s:Activity Id="a3" Type="JSDL">
    <s:JSDL>
      <jsdl:JobDescription>
        <jsdl:Application>
          <jsdl:ApplicationName>Date</jsdl:ApplicationName>
        </jsdl:Application>
      </jsdl:JobDescription>
    </s:JSDL>
   </s:Activity>
   
   <!-- attempt to broker all jobs to the same site-->
   <s:Option name="COBROKER">true</s:Option>
   
  </s:SubWorkflow> 

 <s:Transition Id="a1-to-s1" From="a1" To="s1"/>

</s:Workflow>