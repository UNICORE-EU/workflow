<s:Workflow xmlns:s="http://www.chemomentum.org/workflow/simple"
          xmlns:jsdl="http://schemas.ggf.org/jsdl/2005/11/jsdl"
>
  <s:Activity Id="a1" Type="JSDL">
   <s:JSDL>
      <jsdl:JobDescription>
        <jsdl:Application>
          <jsdl:ApplicationName>Date</jsdl:ApplicationName>
        </jsdl:Application>
      </jsdl:JobDescription>
      <jsdl:DataStaging>
        <jsdl:FileName>stdout</jsdl:FileName>
        <jsdl:Target>
          <jsdl:URI>c9m:stdout</jsdl:URI>
        </jsdl:Target>
      </jsdl:DataStaging>
    </s:JSDL>
   </s:Activity>
  <s:SubWorkflow Id="sub1">
  <s:Activity Id="sub1-a1" Type="JSDL">
   <s:JSDL>
      <jsdl:JobDescription>
        <jsdl:Application>
          <jsdl:ApplicationName>Date</jsdl:ApplicationName>
        </jsdl:Application>
      </jsdl:JobDescription>
    </s:JSDL>
   </s:Activity>
   <s:Activity Id="sub1-a2" Type="JSDL">
   <s:JSDL>
      <jsdl:JobDescription>
        <jsdl:Application>
          <jsdl:ApplicationName>Date</jsdl:ApplicationName>
        </jsdl:Application>
      </jsdl:JobDescription>
    </s:JSDL>
   </s:Activity>
   <s:Transition From="sub1-a1" To="sub1-a2" Id="sub1-a1-sub1-a2"/>
  </s:SubWorkflow>
 <s:Activity Id="a3" Type="JSDL">
   <s:JSDL>
      <jsdl:JobDescription>
        <jsdl:Application>
          <jsdl:ApplicationName>Date</jsdl:ApplicationName>
        </jsdl:Application>
      </jsdl:JobDescription>
    </s:JSDL>
   </s:Activity>

  <s:Transition From="a1" To="sub1" Id="a1-sub1"/>
  <s:Transition From="sub1" To="a3" Id="sub1-a3"/>

</s:Workflow>