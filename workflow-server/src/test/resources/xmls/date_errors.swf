<Workflow xmlns="http://www.chemomentum.org/workflow/simple"
          xmlns:jsdl="http://schemas.ggf.org/jsdl/2005/11/jsdl"
          xmlns:jsdl1="http://schemas.ggf.org/jsdl/2005/11/jsdl-posix"
>
  <Documentation>
    <Comment>Contains some errors</Comment>
  </Documentation>
  
  <Activity Id="date1" Type="JSDL">
   <JSDL>
      <jsdl:JobDescription>
        <jsdl:Application>
          <jsdl:ApplicationName>Date</jsdl:ApplicationName>
        </jsdl:Application>
      </jsdl:JobDescription>
    </JSDL>
    <Option name="SPLIT">false</Option>
    <Option name="MAX_SPLIT">10</Option>
    <Option name="NO_SUCH_OPTION">some value</Option>
   </Activity>

  <Activity Id="date2" Name="NOSUCHTYPE">
   <JSDL>
      <jsdl:JobDescription>
        <jsdl:Application>
          <jsdl:ApplicationName>Date</jsdl:ApplicationName>
        </jsdl:Application>
      </jsdl:JobDescription>
    </JSDL>
    <Option name="NO_SUCH_OPTION">some value</Option>
   </Activity>

   <Transition Id="MissingTarget" From="date1" To="some_target"/>
 
</Workflow>