<s:Workflow xmlns:s="http://www.chemomentum.org/workflow/simple" xmlns:jsdl="http://schemas.ggf.org/jsdl/2005/11/jsdl" xmlns:jsdl1="http://schemas.ggf.org/jsdl/2005/11/jsdl-posix">
  <s:Activity Id="splitme" Type="JSDL">
    <s:JSDL>
      <jsdl:JobDescription>
      <jsdl:JobIdentification><jsdl:JobName>Mopac</jsdl:JobName></jsdl:JobIdentification>
        <jsdl:Application>
          <jsdl:ApplicationName>CMOPAC</jsdl:ApplicationName>
          <jsdl:ApplicationVersion>7.0</jsdl:ApplicationVersion>
          <jsdl1:POSIXApplication>
            <jsdl1:Environment name="INPUT">input.slf</jsdl1:Environment>
            <jsdl1:Environment name="mopacKeywords">AM1 EF GNORM=0.01 NOINTER</jsdl1:Environment>
            <jsdl1:Environment name="OUTPUT">output.slf</jsdl1:Environment>
            <jsdl1:Environment name="ERROR">errorlog</jsdl1:Environment>
          </jsdl1:POSIXApplication>
        </jsdl:Application>
        <jsdl:DataStaging>
          <jsdl:FileName>input.slf</jsdl:FileName>
          <jsdl:Source>
            <jsdl:URI>RBYTEIO:https://zam461.zam.kfa-juelich.de:9130/FZJ-VSITE1/services/StorageManagement?res=default_storage#structures/input2d.slf</jsdl:URI>
          </jsdl:Source>
        </jsdl:DataStaging>
        <jsdl:DataStaging>
          <jsdl:FileName>output.slf</jsdl:FileName>
          <jsdl:Target>
            <jsdl:URI>c9m:/mopac/output.slf</jsdl:URI>
          </jsdl:Target>
         </jsdl:DataStaging>
         <jsdl:DataStaging>
          <jsdl:FileName>stdout</jsdl:FileName>
          <jsdl:Target>
            <jsdl:URI>c9m:/mopac/stdout</jsdl:URI>
          </jsdl:Target>
        </jsdl:DataStaging>
      </jsdl:JobDescription>
    </s:JSDL>
    <!-- enable and configure splitting for this activity -->
    <s:Option name="SPLIT">true</s:Option>
    <s:Option name="MAX_SPLIT">2</s:Option>
  </s:Activity>
 </s:Workflow>
