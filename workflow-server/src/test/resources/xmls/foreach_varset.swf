<s:Workflow xmlns:s="http://www.chemomentum.org/workflow/simple" 
            xmlns:jsdl="http://schemas.ggf.org/jsdl/2005/11/jsdl" 
            xmlns:jsdl1="http://schemas.ggf.org/jsdl/2005/11/jsdl-posix"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

<s:SubWorkflow Id="for1" xsi:type="s:ForEachType" IteratorName="ForEachIterator">

<s:SubWorkflow Id="for1_body">
  <s:Activity Id="RunScript" Type="JSDL">
    <s:JSDL>
      <jsdl:JobDescription>
        <jsdl:Application>
           <jsdl:ApplicationName>Echo</jsdl:ApplicationName>
           <jsdl1:POSIXApplication>
            <jsdl1:Argument>${CURRENT_ITERATOR_VALUE}</jsdl1:Argument>
          </jsdl1:POSIXApplication>
        </jsdl:Application>
      </jsdl:JobDescription>
    </s:JSDL>
  </s:Activity>
</s:SubWorkflow>

  <s:VariableSet>
      <s:VariableName>C</s:VariableName>
      <s:Type>INTEGER</s:Type>
      <s:StartValue>0</s:StartValue>
      <s:Expression>C++</s:Expression>
      <s:EndCondition>C&lt;5</s:EndCondition>
  </s:VariableSet>
  
 </s:SubWorkflow>

</s:Workflow>